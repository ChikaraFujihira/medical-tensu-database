{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "t-specific-materials (特定保険医療材料マスター)",
  "type": "object",
  "properties": {
    "material_code": {
      "type": "string",
      "description": "特定保険医療材料コード（9桁前後）。"
    },
    "name": {
      "type": "string",
      "description": "材料名（規格名称を含む）。"
    },
    "kana": {
      "type": "string",
      "description": "名称カナ表記。"
    },
    "category_large": {
      "type": "string",
      "description": "材料区分（大分類）。"
    },
    "category_middle": {
      "type": "string",
      "description": "材料区分（中分類）。"
    },
    "category_small": {
      "type": "string",
      "description": "材料区分（小分類）。"
    },
    "price": {
      "type": "number",
      "description": "材料価格（円）。"
    },
    "unit": {
      "type": "string",
      "description": "材料単位（個、mL、セット等）。"
    },
    "valid_from": {
      "type": "string",
      "format": "date",
      "description": "適用開始日。"
    },
    "valid_to": {
      "type": "string",
      "format": "date",
      "description": "適用終了日。"
    },
    "change_type": {
      "type": "string",
      "description": "変更区分（新規、変更、廃止など）。"
    },
    "maker": {
      "type": "string",
      "description": "メーカー名。"
    },
    "model": {
      "type": "string",
      "description": "型番、規格等。"
    },
    "raw_record": {
      "type": "string",
      "description": "元TSVファイル1行分（UTF-8）。"
    }
  },
  "required": [
    "material_code",
    "name",
    "price",
    "valid_from",
    "raw_record"
  ]
}
